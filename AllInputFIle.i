                                                   # Group: Advanced
        debug_variable     = (required)            # The variable that is being debugged.
        enable             = 1                     # Set the enabled status of the MooseObject.
                                                   # Group: Advanced
        execute_on         = 'LINEAR TIMESTEP_END' # The list of flag(s) indicating when this object should
                                                   # be executed, the available options include NONE, INITIAL,
                                                   # LINEAR, NONLINEAR, TIMESTEP_END, TIMESTEP_BEGIN, FINAL,
